##long approch
class Solution {
    public int countDigitOne(int n) {
        int count=0;
        if(n<1){
            return count;
        }
        for(int i=1;i<=n;i++){
            int num=i;
            while(num>0){
                if(num%10==1){
                    count+=1;
                }
                num/=10;
            }
        }
        return count;
    } 
}   

##optimal approch 



